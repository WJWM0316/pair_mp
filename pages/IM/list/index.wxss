.listWrap {
  width: 100%;
  box-sizing: border-box;
  background: #fff;
}
.listWrap .tips {
  height: 295rpx;
  background: #0CCDDA;
  margin-bottom: -156rpx;
  padding: 25rpx 202rpx 0 40rpx;
  box-sizing: border-box;
  position: relative;
}
.listWrap .tips .text {
  font-size: 26rpx;
  color: #fff;
  line-height: 34rpx;
}
.listWrap .tips .close {
  width: 42rpx;
  height: 46rpx;
  background: rgba(255, 255, 255, 0.33);
  border-radius: 52rpx 0rpx 0rpx 52rpx;
  color: rgba(255, 255, 255, 0.8);
  font-size: 14rpx;
  padding-left: 17rpx;
  box-sizing: border-box;
  line-height: 46rpx;
  display: block;
  position: absolute;
  top: 46rpx;
  right: 0;
}
.listWrap .noTips {
  height: 40rpx;
  margin-bottom: -20rpx;
  background: #EDF1F0;
}
.listWrap .scroll-view {
  padding-top: 20rpx;
  border-radius: 36rpx 36rpx 0px 0px;
  overflow: hidden;
  background: #fff;
  position: relative;
}
.listWrap .scroll-view .scroll-item {
  padding: 28rpx 40rpx 28rpx 170rpx;
  width: 100%;
  position: relative;
  box-sizing: border-box;
}
.listWrap .scroll-view .scroll-item .avatar {
  width: 100rpx;
  height: 100rpx;
  box-shadow: 0px 2rpx 20rpx 0rpx rgba(42, 63, 64, 0.1);
  position: absolute;
  top: 28rpx;
  left: 40rpx;
  border-radius: 50%;
}
.listWrap .scroll-view .scroll-item .avatar.online::after {
  position: absolute;
  right: 4rpx;
  bottom: 4rpx;
  width: 20rpx;
  height: 20rpx;
  background: #0CCDDA;
  box-shadow: 0px 0px 6px 0px #0CCDDA;
  border: 2rpx solid #fff;
  display: block;
  content: '';
  border-radius: 50%;
}
.listWrap .scroll-view .scroll-item .avatar .avatarImg {
  border-radius: 50%;
}
.listWrap .scroll-view .scroll-item .messageContent {
  width: 100%;
  height: 100rpx;
  position: relative;
}
.listWrap .scroll-view .scroll-item .messageContent .name {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  word-wrap: normal;
  padding: 8rpx 0;
  font-size: 32rpx;
  font-weight: 700;
  color: #1F252B;
  line-height: 40rpx;
}
.listWrap .scroll-view .scroll-item .messageContent .name .time {
  float: right;
  font-size: 24rpx;
  font-weight: 300;
  color: #9CA1AA;
}
.listWrap .scroll-view .scroll-item .messageContent .name .label {
  display: inline-block;
  width: 68rpx;
  height: 30rpx;
  text-align: center;
  line-height: 30rpx;
  padding: 5rpx 0;
  background: #FDF0F5;
  border-radius: 15rpx 15rpx 15rpx 0rpx;
  font-size: 20rpx;
  font-weight: 700;
  color: #F46BA1;
  margin-left: 16rpx;
  vertical-align: top;
}
.listWrap .scroll-view .scroll-item .messageContent .content {
  width: 481rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  word-wrap: normal;
  font-size: 26rpx;
  font-weight: 300;
  color: #66666E;
  line-height: 34pxr;
}
.listWrap .scroll-view .scroll-item .messageContent .messageNum {
  position: absolute;
  width: 36rpx;
  height: 36rpx;
  background: #E85274;
  right: 0rpx;
  bottom: 9rpx;
  border-radius: 50%;
  font-size: 22rpx;
  font-weight: 300;
  color: #fff;
  line-height: 36rpx;
  text-align: center;
}
.listWrap .scroll-view .rightBtn .closeBtn {
  width: 100rpx;
  height: 100rpx;
  background: #E85274;
  border-radius: 50%;
  margin: 28rpx 20rpx 28rpx 10rpx;
  border: none;
}
.listWrap .scroll-view .rightBtn .closeBtn .closeMessage {
  color: #fff;
  font-size: 32rpx;
  line-height: 100rpx;
}
