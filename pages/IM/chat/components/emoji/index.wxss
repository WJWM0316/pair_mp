.emojiWrap {
  width: 100%;
  height: 100%;
  box-sizing: border-box;
  padding: 50rpx 55rpx 0;
  overflow-y: auto;
}
.emojiWrap .list {
  overflow: hidden;
}
.emojiWrap .list .emoji {
  padding-right: 17rpx;
  padding-bottom: 20rpx;
  float: left;
}
.emojiWrap .list .emoji:nth-child(8n) {
  padding-right: 0;
}
.emojiWrap .list .emoji .emojiImg {
  width: 66rpx;
  height: 66rpx;
}
