.loginWrap {
  padding-top: 374rpx;
}
.loginWrap .box {
  width: 560rpx;
  height: 98rpx;
  background: #fff;
  border-radius: 49rpx;
  margin: 0 auto 24rpx;
  padding: 23rpx 40rpx 23rpx 112rpx;
  box-sizing: border-box;
}
.loginWrap .box .input {
  width: 100%;
  height: 100%;
  color: #1F252B;
}
.loginWrap .loginBtn {
  width: 560rpx;
  height: 98rpx;
  background: #9FE9F0;
  border-radius: 49rpx;
  margin: 78rpx auto 0;
  color: #fff;
  font-size: 32rpx;
  border: none;
  line-height: 98rpx;
}
