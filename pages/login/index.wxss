.loginWrap {
  padding-top: 374rpx;
}
.loginWrap .box {
  width: 560rpx;
  height: 98rpx;
  background: #fff;
  border-radius: 49rpx;
  margin: 0 auto 24rpx;
  padding: 23rpx 50rpx 23rpx 112rpx;
  box-sizing: border-box;
  position: relative;
}
.loginWrap .box.codeBox {
  padding: 23rpx 190rpx 23rpx 112rpx;
}
.loginWrap .box .input {
  width: 100%;
  height: 100%;
  color: #1F252B;
}
.loginWrap .box .send {
  position: absolute;
  top: 30rpx;
  right: 50rpx;
  font-size: 28rpx;
  font-weight: 700;
  color: #0CCDDA;
  line-height: 38rpx;
}
.loginWrap .loginBtn {
  width: 560rpx;
  height: 98rpx;
  background: #9FE9F0;
  border-radius: 49rpx;
  margin: 78rpx auto 0;
  color: #fff;
  font-size: 32rpx;
  border: none;
  line-height: 98rpx;
}
.loginWrap .loginBtn.canClick {
  background: #0CCDDA;
}
