.settingWrap {
  padding: 30rpx 0;
}
.settingWrap .item {
  position: relative;
}
.settingWrap .item .item-inner {
  padding-left: 60rpx;
  height: 108rpx;
  line-height: 108rpx;
  background: #fff;
}
.settingWrap .item .item-inner::before,
.settingWrap .item .item-inner::after {
  content: " ";
  display: table;
}
.settingWrap .item .item-inner::after {
  clear: both;
}
.settingWrap .item .item-inner .name {
  font-size: 30rpx;
  font-weight: 400;
  color: #1F252B;
  display: inline-block;
  float: left;
}
.settingWrap .item .item-inner .value {
  font-size: 30rpx;
  color: #66666E;
  display: inline-block;
  float: right;
  padding-right: 59rpx;
  text-align: right;
}
.settingWrap .item .item-inner .value .icon {
  font-size: 20rpx;
  margin-left: 21rpx;
  vertical-align: top;
  line-height: 108rpx;
  height: 108rpx;
  display: inline-block;
}
.settingWrap .item:after {
  content: '';
  width: 690rpx;
  height: 2rpx;
  background: #EDF1F0;
  position: absolute;
  right: 0;
  bottom: 0;
  transform: scaleY(0.5);
}
.settingWrap .item.margin {
  margin-top: 24rpx;
}
.settingWrap .logout {
  width: 100%;
  height: 98rpx;
  line-height: 98rpx;
  border: none;
  background: #fff;
  color: #E85274;
  font-size: 30rpx;
  box-shadow: none;
}
