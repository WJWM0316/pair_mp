.navWrap {
  width: 100%;
  position: fixed;
  top: 0;
  left: 0;
}
.navWrap .nav-content {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 0 40rpx;
}
.navWrap .nav-content .left,
.navWrap .nav-content .right {
  flex: 1;
}
.navWrap .nav-content .right {
  background: #000;
}
.navWrap .nav-content .center {
  flex: 2;
  text-align: center;
  font-size: 36rpx;
  color: #1F252B;
  font-weight: 700;
}
