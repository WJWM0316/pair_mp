.tabBar {
  width: 100%;
  position: fixed;
  left: 0;
  bottom: 0;
  padding-bottom: 30rpx;
}
.tabBar .tabBar-content {
  width: 686rpx;
  height: 118rpx;
  background: #fff;
  box-shadow: 0rpx 12rpx 30rpx 0rpx rgba(30, 102, 102, 0.1);
  border-radius: 60rpx;
  margin: 0 auto;
  padding: 0 116rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  align-items: center;
  position: relative;
  z-index: 2;
}
.tabBar .tabBar-content .icon {
  display: block;
  color: #C7D7E7;
}
.tabBar .tabBar-content .text {
  display: block;
  color: #C7D7E7;
  font-size: 22rpx;
  line-height: 30rpx;
  font-weight: 500;
}
.tabBar .tabBar-mask {
  width: 100%;
  height: 88rpx;
  position: absolute;
  left: 0;
  bottom: 0;
  z-index: 1;
  background: linear-gradient(180deg, rgba(255, 255, 255, 0.4) 0%, #ffffff 100%);
}
.block {
  width: 100%;
  height: 178rpx;
  display: block;
}
@supports (bottom: constant(safe-area-inset-bottom)) or (bottom: env(safe-area-inset-bottom)) {
  .tabBar {
    padding-bottom: calc(env(safe-area-inset-bottom) + 30rpx);
    padding-bottom: calc(constant(safe-area-inset-bottom) + 30rpx);
  }
  .block {
    height: calc(env(safe-area-inset-bottom) + 178rpx);
    height: calc(constant(safe-area-inset-bottom) + 178rpx);
  }
}
