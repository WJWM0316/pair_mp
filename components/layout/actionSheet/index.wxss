.drawer {
  width: 100vw;
  height: 100vh;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 10;
}
.drawer .photo-box1 {
  background: #ffffff;
  border-radius: 24rpx;
  position: absolute;
  left: 87rpx;
  right: 87rpx;
  z-index: 9;
  box-sizing: border-box;
  padding: 40rpx;
  bottom: 420rpx;
}
.drawer .photo-box1 .h11 {
  height: 44rpx;
  font-size: 36rpx;
  font-weight: 700;
  color: #1F252B;
  line-height: 44rpx;
  text-align: center;
}
.drawer .photo-box1 .example {
  height: 283rpx;
  background: #ede8f1;
  border-radius: 16rpx;
  margin: 20rpx 0;
  position: relative;
  overflow: hidden;
}
.drawer .photo-box1 .example .cover-img {
  width: 100%;
  height: 100%;
}
.drawer .photo-box1 .p11 {
  height: 36rpx;
  font-size: 28rpx;
  color: #1f252b;
  line-height: 36rpx;
}
.drawer .photo-box1 .p111 {
  height: 36rpx;
  font-size: 28rpx;
  color: #1f252b;
  line-height: 36rpx;
  margin-top: 14rpx;
  padding-bottom: 30rpx;
  position: relative;
}
.drawer .photo-box1 .p111:after {
  position: absolute;
  left: 0;
  right: 0;
  bottom: 0;
  content: '';
  height: 1rpx;
  transform: scaleY(0.5);
  background: #EDF1F0;
  display: block;
}
.drawer .photo-box1 .list {
  overflow: hidden;
  margin-top: 30rpx;
}
.drawer .photo-box1 .item1 {
  width: 142rpx;
  display: inline-block;
  text-align: center;
}
.drawer .photo-box1 .item1:nth-child(2n) {
  margin: 0 35rpx;
}
.drawer .photo-box1 .item1 .box {
  width: 142rpx;
  height: 142rpx;
  border-radius: 16rpx;
  background: rgba(0, 0, 0, 0.05);
  position: relative;
  overflow: hidden;
}
.drawer .photo-box1 .item1 .cover-img {
  width: 100%;
  height: 100%;
}
.drawer .photo-box1 .text {
  height: 34rpx;
  font-size: 26rpx;
  color: #66666E;
  line-height: 34rpx;
  margin-top: 14rpx;
}
.drawer .photo-box1 .action {
  width: 30rpx;
  height: 30rpx;
  border-radius: 50%;
  border: 2rpx solid #fff;
  position: absolute;
  bottom: 14rpx;
  right: 14rpx;
  background: #E85274;
  color: #fff;
  text-align: center;
  line-height: 30rpx;
}
.drawer .photo-box1 .action .icon {
  font-size: 12rpx;
  line-height: 1;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
}
.drawer .photo-box1 .icon2 {
  width: 50rpx;
  height: 50rpx;
  border-radius: 50%;
  border: 4rpx solid #fff;
  bottom: 20rpx;
  right: 20rpx;
  color: #0CCDDA;
  text-align: center;
  line-height: 50rpx;
  font-size: 50rpx;
  position: absolute;
}
.drawer .photo-box1 .icon3 {
  font-size: 18rpx;
  color: #1F252B;
  position: relative;
  top: -3rpx;
  margin-right: 5rpx;
}
.drawer .content {
  width: 100vw;
  height: auto;
  max-height: calc(100vh - 224rpx);
  border-radius: 24rpx 24rpx 0rpx 0rpx;
  position: absolute;
  bottom: 0;
  left: 0;
  box-sizing: border-box;
  z-index: 10;
  overflow: hidden;
  transition: all 0.4s linear;
  opacity: 0.5;
  background: #F1F4F4;
  transform: translateY(-100%);
  z-index: 11;
}
.drawer .cover-image {
  position: fixed;
  z-index: 10;
  width: 534rpx;
  left: 50%;
  transform: translateX(-50%);
  bottom: 470rpx;
}
.drawer .mask {
  width: 100vw;
  height: 100vh;
  position: fixed;
  top: 0;
  left: 0;
  background: rgba(0, 0, 0, 0.8);
  z-index: 9;
  animation-name: modalopen;
  animation-duration: 0.2s;
}
.drawer .mask.show {
  bottom: 0;
  opacity: 1;
}
.drawer .content.show {
  bottom: 0;
  opacity: 1;
  transform: translateY(0);
}
.drawer .header {
  width: 100%;
  height: 108rpx;
  background: #ffffff;
  font-size: 36rpx;
  font-weight: 700;
  color: #354048;
  line-height: 108rpx;
  text-align: center;
  position: relative;
  margin-bottom: 30rpx;
  padding: 0 60rpx;
  box-sizing: border-box;
}
.drawer .header .icon-close {
  font-size: 30rpx;
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  right: 10rpx;
  padding: 0 25rpx;
  color: #929292;
}
.drawer .item {
  height: 112rpx;
  background: #ffffff;
  position: relative;
  line-height: 112rpx;
  text-align: center;
  font-size: 32rpx;
  color: #1F252B;
  background: white;
}
.drawer .item:after {
  position: absolute;
  left: 0;
  right: 0;
  bottom: 0;
  content: '';
  height: 1rpx;
  transform: scaleY(0.5);
  background: #EDF1F0;
  display: block;
}
.drawer .item:last-child {
  margin-top: 16rpx;
}
.drawer .delete {
  color: #E85274;
}
.drawer .no-line {
  margin-bottom: 16rpx;
  color: #E85274;
}
.drawer .no-line:after {
  position: absolute;
  display: none;
}
@keyframes modalopen {
  from {
    opacity: 0.5;
  }
  to {
    opacity: 1;
  }
}
