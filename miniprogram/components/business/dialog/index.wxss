.modal-dialog {
  transition: all ease 0.2s;
  z-index: 12;
  position: relative;
  width: 100vw;
  height: 100vh;
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  box-sizing: border-box;
  opacity: 0;
  display: none;
  z-index: 1000;
}
.modal-dialog.show {
  display: block;
  opacity: 1;
}
.modal-dialog .mask {
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  top: 0;
  background: rgba(0, 0, 0, 0.5);
  opacity: 0;
  z-index: 9;
  animation-name: modalopen;
  animation-duration: 0.2s;
}
.modal-dialog .mask.show {
  opacity: 1;
}
.modal-dialog .content {
  width: 650rpx;
  background: #ffffff;
  border-radius: 36rpx;
  position: fixed;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  z-index: 10;
}
.modal-dialog .model_title {
  font-size: 36rpx;
  font-weight: 700;
  color: #1F252B;
  line-height: 46rpx;
  text-align: center;
  margin: 80rpx 90rpx 0 90rpx;
}
.modal-dialog .icon_xiaocha {
  position: absolute;
  top: 0;
  right: 0;
  font-size: 20rpx;
  color: #CFD4D8;
  padding: 38rpx;
}
.modal-dialog .p1 {
  height: 38rpx;
  font-size: 30rpx;
  font-weight: 300;
  color: #1F252B;
  line-height: 38rpx;
  text-align: center;
  margin-top: 36rpx;
}
.modal-dialog .sex-ul {
  height: 280rpx;
  padding: 24rpx 75rpx 48rpx 75rpx;
}
.modal-dialog .sex-ul .li {
  width: 242rpx;
  height: 280rpx;
  border-radius: 36rpx;
  display: inline-block;
  line-height: 280rpx;
  text-align: center;
}
.modal-dialog .sex-ul .li.boy {
  background: rgba(12, 205, 218, 0.08);
  margin-right: 16rpx;
  color: #0CCDDA;
}
.modal-dialog .sex-ul .li.girl {
  background: rgba(244, 107, 161, 0.08);
  color: #F46BA1;
}
.modal-dialog .sex-ul .icon {
  margin-right: 16rpx;
  font-size: 30rpx;
}
.modal-dialog .p2 {
  height: 36rpx;
  font-size: 28rpx;
  color: #1F252B;
  line-height: 36rpx;
  text-align: center;
  padding-bottom: 90rpx;
}
.modal-dialog .p2 text {
  text-decoration: underline;
}
.modal-dialog .process {
  padding: 0 60rpx;
  margin: 38rpx 0 48rpx 0;
}
.modal-dialog .process .bar {
  height: 10rpx;
  background: #F1F4F4;
  border-radius: 5rpx;
  position: relative;
}
.modal-dialog .process .active1 {
  height: 100%;
  background: #E85274;
  border-radius: 5rpx;
}
.modal-dialog .process .active2 {
  height: 100%;
  background: #0CCDDA;
  border-radius: 5rpx;
}
.modal-dialog .process .tips {
  font-size: 26rpx;
  color: #1F252B;
  line-height: 32rpx;
  font-weight: 300;
  margin-top: 30rpx;
}
.modal-dialog .process .icon-image {
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  left: 80%;
  color: #F46BA1;
  width: 26rpx;
  height: 26rpx;
}
.modal-dialog .perfect-ul {
  padding: 0 40rpx 80rpx 40rpx;
}
.modal-dialog .perfect-ul .li {
  height: 110rpx;
  background: #F1F4F4;
  border-radius: 55rpx;
  position: relative;
  padding-left: 50rpx;
  padding-right: 36rpx;
  line-height: 110rpx;
  margin-bottom: 24rpx;
}
.modal-dialog .perfect-ul .li:last-child {
  margin-bottom: 0rpx;
}
.modal-dialog .perfect-ul .text {
  color: #1F252B;
  padding-left: 16rpx;
  font-size: 30rpx;
}
.modal-dialog .perfect-ul .btn {
  width: 138rpx;
  height: 60rpx;
  border-radius: 33rpx;
  text-align: center;
  line-height: 60rpx;
  font-size: 28rpx;
  font-weight: 700;
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  right: 36rpx;
  color: #0CCDDA;
}
.modal-dialog .perfect-ul .btn.bg {
  background: #0CCDDA;
  color: #ffffff;
}
.modal-dialog .perfect-ul .icon {
  font-size: 30rpx;
  color: #1F252B;
}
.modal-dialog .perfect-ul2 {
  padding: 48rpx 40rpx 80rpx 40rpx;
}
.modal-dialog .perfect-ul2 .li {
  height: 110rpx;
  background: rgba(241, 244, 244, 0.46);
  border-radius: 55rpx;
  position: relative;
  padding-left: 50rpx;
  padding-right: 36rpx;
  line-height: 110rpx;
  margin-bottom: 24rpx;
}
.modal-dialog .perfect-ul2 .li:last-child {
  margin-bottom: 0rpx;
}
.modal-dialog .perfect-ul2 .text {
  color: #1F252B;
  padding-left: 16rpx;
  font-size: 30rpx;
}
.modal-dialog .perfect-ul2 .btn {
  width: 138rpx;
  height: 60rpx;
  border-radius: 33rpx;
  text-align: center;
  line-height: 60rpx;
  font-size: 28rpx;
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  right: 36rpx;
  color: #0CCDDA;
}
.modal-dialog .perfect-ul2 .btn.bg {
  background: #E85274;
  color: #ffffff;
  font-weight: 700;
}
.modal-dialog .perfect-ul2 .icon1 {
  font-size: 30rpx;
  color: #0CCDDA;
}
.modal-dialog .perfect-ul2 .icon2 {
  font-size: 30rpx;
  color: #0CCDDA;
}
.modal-dialog .perfect-ul2 .btn2 {
  width: 138rpx;
  height: 60rpx;
  border-radius: 33rpx;
  text-align: center;
  line-height: 60rpx;
  font-size: 28rpx;
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  right: 36rpx;
  color: #9CA1AA;
}
.modal-dialog .perfect-ul2 .icon1_error {
  color: #E85274;
}
.modal-dialog .error {
  width: 82rpx;
  height: 36rpx;
  background: rgba(232, 82, 116, 0.1);
  border-radius: 8rpx;
  font-size: 22rpx;
  color: #E85274;
  text-align: center;
  line-height: 36rpx;
  margin-left: 10rpx;
  display: inline-block;
  vertical-align: middle;
  position: relative;
  top: -2rpx;
}
.modal-dialog .charge-box {
  width: 650rpx;
  background: #ffffff;
  border-radius: 36rpx;
}
.modal-dialog .charge-box .headImg {
  width: 522rpx;
  height: 168rpx;
  border-radius: 250rpx;
  border: 1rpx solid;
  margin: 60rpx auto 0;
  display: block;
  border-image: linear-gradient(180deg, rgba(255, 255, 255, 0), rgba(12, 205, 218, 0.69), rgba(12, 205, 218, 0.65), rgba(255, 255, 255, 0)) 1 1;
}
.modal-dialog .charge-box .img-box_v1 {
  height: 168rpx;
  margin-top: 60rpx;
  text-align: center;
  position: relative;
}
.modal-dialog .charge-box .circle_v1 {
  width: 168rpx;
  height: 168rpx;
  border: 3rpx solid #0ccdda;
  border-radius: 50%;
  box-sizing: border-box;
  display: inline-block;
  overflow: hidden;
  position: relative;
  vertical-align: top;
  position: absolute;
}
.modal-dialog .charge-box .circle_v1:first-child {
  transform: translateX(-100%);
}
.modal-dialog .charge-box .circle_v1:last-child {
  transform: translateX(-100%);
}
.modal-dialog .charge-box .circle_v1.girl {
  border-color: #F46BA1;
}
.modal-dialog .charge-box .circle_v1.boy {
  border-color: #0CCDDA;
}
.modal-dialog .charge-box .circle_v1 .use_avatar {
  width: 100%;
  height: 100%;
}
.modal-dialog .charge-box .love_v1 {
  width: 110rpx;
  height: 110rpx;
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
  border-radius: 50%;
  text-align: center;
}
.modal-dialog .charge-box .model_title_v2 {
  height: 44rpx;
  font-size: 36rpx;
  font-weight: 700;
  color: #1F252B;
  line-height: 44rpx;
  margin-top: 60rpx;
  text-align: center;
}
.modal-dialog .charge-box .p_v2 {
  height: 38rpx;
  font-size: 30rpx;
  font-weight: 300;
  color: #1F252B;
  line-height: 38rpx;
  text-align: center;
  margin-top: 20rpx;
}
.modal-dialog .charge-box .row-botton {
  height: 68rpx;
  border-radius: 34rpx;
  position: relative;
  margin: 20rpx 119rpx 64rpx 119rpx;
  line-height: 68rpx;
}
.modal-dialog .charge-box .row-botton:after {
  content: '';
  display: block;
  position: absolute;
  left: 0;
  right: 0;
  bottom: 0;
  top: 0;
  background: #0CCDDA;
  border-radius: 34rpx;
  opacity: 0.09;
}
.modal-dialog .charge-box .row-botton .pitang {
  position: absolute;
  left: 0;
  top: 50%;
  transform: translateY(-50%);
  background: #0CCDDA;
  width: 60rpx;
  height: 60rpx;
  box-shadow: 0rpx 0rpx 24rpx 0rpx rgba(244, 107, 161, 0.3);
  border-radius: 50%;
  box-sizing: border-box;
  padding: 8rpx;
}
.modal-dialog .charge-box .text_v1 {
  font-size: 28rpx;
  font-weight: 300;
  color: #1F252B;
  padding-left: 80rpx;
}
.modal-dialog .charge-box .strong_v1 {
  padding: 0 5rpx;
  color: #0CCDDA;
  font-weight: 700;
}
.modal-dialog .charge-box .action-button_v1 {
  text-align: center;
  position: relative;
  margin: 0 70rpx 64rpx 70rpx;
}
.modal-dialog .charge-box .btn_v1 {
  height: 98rpx;
  border-radius: 49rpx;
  line-height: 98rpx;
  text-align: center;
  display: inline-block;
  border: 0;
  width: 100%;
}
.modal-dialog .charge-box .btn_v1:before {
  height: 88rpx;
  background: linear-gradient(180deg, #1ce8ef 0%, #0ccdda 100%);
  border-radius: 44rpx;
  opacity: 0.89;
  filter: blur(10rpx);
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  bottom: -7rpx;
  content: '';
  display: block;
  border: 0;
  width: 430rpx;
}
.modal-dialog .charge-box .btn_cover_v1 {
  height: 98rpx;
  background: linear-gradient(180deg, #1ce8ef 0%, #0ccdda 100%);
  border-radius: 49rpx;
  line-height: 98rpx;
  text-align: center;
  font-size: 32rpx;
  font-weight: 700;
  color: #ffffff;
  position: relative;
  z-index: 2;
  width: 100%;
}
.modal-dialog .charge-box .btn_v2 {
  background: linear-gradient(180deg, #1ce8ef 0%, #0ccdda 100%);
  border-radius: 44rpx;
  text-align: center;
  flex: 0 0 0 197rpx;
  font-size: 30rpx;
  font-weight: 700;
  color: #ffffff;
  padding: 0;
  width: 510rpx;
  height: 98rpx;
  border-radius: 50rpx;
  line-height: 98rpx;
}
@keyframes modalopen {
  from {
    opacity: 0.5;
  }
  to {
    opacity: 1;
  }
}
