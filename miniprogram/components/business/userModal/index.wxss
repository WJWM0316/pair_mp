.mine-modal {
  transform: translateY(100%);
  transition: all ease 0.2s;
  z-index: 12;
  position: relative;
  width: 100vw;
  height: 100vh;
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  box-sizing: border-box;
}
.mine-modal.show {
  transform: translateY(0);
}
.mine-modal .mask {
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  top: 0;
  background: rgba(0, 0, 0, 0.9);
  opacity: 0;
  z-index: 9;
  animation-name: modalopen;
  animation-duration: 0.2s;
}
.mine-modal .mask.show {
  opacity: 1;
}
.mine-modal .base {
  display: flex;
  height: 148rpx;
  align-items: center;
  position: relative;
  padding: 0 80rpx;
}
.mine-modal .base .content {
  position: relative;
  flex: 1;
}
.mine-modal .base .h1 {
  height: 50rpx;
  font-size: 42rpx;
  font-weight: 700;
  color: #1F252B;
  line-height: 50rpx;
}
.mine-modal .base .h2 {
  height: 32rpx;
  font-size: 28rpx;
  font-weight: 700;
  color: #1F252B;
  line-height: 32rpx;
  margin-top: 18rpx;
}
.mine-modal .base .avatar {
  width: 148rpx;
  height: 148rpx;
  position: relative;
  border-radius: 50%;
  margin-right: 30rpx;
  flex: 0 0 148rpx;
  overflow: hidden;
}
.mine-modal .base .avatar:after {
  content: '';
  position: absolute;
  width: 200%;
  height: 200%;
  border: 1px solid #F1F4F4;
  transform-origin: 0 0;
  transform: scale(0.5, 0.5);
  box-sizing: border-box;
  border-radius: calc(2 * 74rpx);
}
.mine-modal .base .cover {
  width: 100%;
  height: 100%;
}
.mine-modal .base .icon_aixin {
  font-size: 26rpx;
  color: #F46BA1;
  margin-right: 12rpx;
}
.mine-modal .icon_jiantou-you- {
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  right: 80rpx;
  font-size: 30rpx;
  color: #CFD4D8;
}
.mine-modal .process {
  padding: 0 80rpx;
  margin: 52rpx 0 34rpx 0;
}
.mine-modal .process .bar {
  height: 10rpx;
  background: #F1F4F4;
  border-radius: 5rpx;
  position: relative;
}
.mine-modal .process .active1 {
  height: 100%;
  background: #E85274;
  border-radius: 5rpx;
}
.mine-modal .process .active2 {
  height: 100%;
  background: #0CCDDA;
  border-radius: 5rpx;
}
.mine-modal .process .tips {
  height: 32rpx;
  font-size: 24rpx;
  color: #66666E;
  line-height: 32rpx;
  margin-top: 15rpx;
}
.mine-modal .bubble {
  height: 160rpx;
  border-radius: 16rpx;
  margin: 0 40rpx 0 40rpx;
  display: flex;
  align-items: center;
  position: relative;
  padding: 0 40rpx;
  box-sizing: border-box;
  margin-bottom: 24rpx;
}
.mine-modal .bubble .bg1 {
  border-radius: 16rpx;
  opacity: 0.04;
  display: flex;
  align-items: center;
  position: absolute;
  left: 0;
  right: 0;
  bottom: 0;
  top: 0;
}
.mine-modal .bubble .bg2 {
  width: 314rpx;
  height: 108rpx;
  position: absolute;
  left: 0;
  bottom: 0;
}
.mine-modal .bubble .btn {
  width: 175rpx;
  height: 60rpx;
  border-radius: 30rpx;
  flex: 175rpx 0 0;
  font-size: 28rpx;
  font-weight: 700;
  color: #ffffff;
  text-align: center;
  line-height: 60rpx;
  position: relative;
}
.mine-modal .bubble .content {
  flex: 1;
}
.mine-modal .bubble .h1 {
  height: 50rpx;
  line-height: 50rpx;
  font-size: 36rpx;
  font-weight: 700;
  color: #1F252B;
}
.mine-modal .bubble .h2 {
  height: 32rpx;
  font-size: 28rpx;
  font-weight: 700;
  line-height: 32rpx;
  margin-top: 3rpx;
  font-size: 24rpx;
  color: #66666E;
}
.mine-modal .bubble.style1 .bg1 {
  background: #E85274;
}
.mine-modal .bubble.style1 .btn {
  background: #E85274;
}
.mine-modal .bubble.style1 .bg2 {
  background: linear-gradient(192deg, rgba(245, 138, 172, 0) 0%, rgba(232, 82, 116, 0.1) 100%);
}
.mine-modal .bubble.style2 .bg1 {
  background: #0CCDDA;
}
.mine-modal .bubble.style2 .btn {
  background: #0CCDDA;
}
.mine-modal .bubble.style2 .bg2 {
  background: linear-gradient(190deg, rgba(255, 255, 255, 0) 0%, rgba(12, 205, 218, 0.42) 100%);
}
.mine-modal .upload {
  height: 160rpx;
  border-radius: 16rpx;
  margin: 0 40rpx 0 40rpx;
  display: flex;
  align-items: center;
  position: relative;
  padding: 0 40rpx;
  box-sizing: border-box;
  margin-bottom: 24px;
}
.mine-modal .upload .bg1 {
  background: #E85274;
  border-radius: 16rpx;
  display: flex;
  align-items: center;
  position: absolute;
  left: 0;
  right: 0;
  bottom: 0;
  top: 0;
  opacity: 0.04;
}
.mine-modal .upload .bg2 {
  width: 314rpx;
  height: 108rpx;
  background: linear-gradient(192deg, rgba(245, 138, 172, 0) 0%, rgba(232, 82, 116, 0.1) 100%);
  position: absolute;
  left: 0;
  bottom: 0;
}
.mine-modal .upload .btn {
  width: 175rpx;
  height: 60rpx;
  background: #E85274;
  border-radius: 30rpx;
  flex: 175rpx 0 0;
  font-size: 28rpx;
  font-weight: 700;
  color: #ffffff;
  text-align: center;
  line-height: 60rpx;
}
.mine-modal .upload .content {
  flex: 1;
}
.mine-modal .upload .h1 {
  height: 50rpx;
  color: #1F252B;
  line-height: 50rpx;
  font-size: 36rpx;
  font-weight: 700;
}
.mine-modal .upload .h2 {
  height: 32rpx;
  font-size: 28rpx;
  font-weight: 700;
  color: #66666E;
  line-height: 32rpx;
  margin-top: 3rpx;
  font-size: 24rpx;
}
.mine-modal .ul {
  overflow: hidden;
  margin-top: 60rpx;
}
.mine-modal .ul .li {
  height: 128rpx;
  line-height: 128rpx;
  position: relative;
  box-sizing: border-box;
  padding: 0 80rpx;
}
.mine-modal .ul .icon1 {
  font-size: 44rpx;
  display: inline-block;
  vertical-align: middle;
  color: #1F252B;
}
.mine-modal .ul .key {
  display: inline-block;
  vertical-align: middle;
  margin-left: 47rpx;
  color: #1F252B;
  font-size: 30rpx;
  font-weight: 700;
}
.mine-modal .ul .value {
  display: inline-block;
  vertical-align: middle;
  margin-left: 47rpx;
  position: absolute;
  right: 122rpx;
  top: 50%;
  transform: translateY(-50%);
  font-size: 26rpx;
  color: #F46BA1;
}
.mine-modal .ul .redot:before {
  width: 12rpx;
  height: 12rpx;
  background: #F46BA1;
  border-radius: 50%;
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  display: block;
  content: '';
  left: -24rpx;
}
.mine-modal .ul .color1 {
  color: #9CA1AA;
}
.mine-modal .overlay {
  position: fixed;
  z-index: 10;
  background: white;
  bottom: 0;
  left: 0;
  right: 0;
  border-radius: 36rpx 36rpx 0rpx 0rpx;
  padding-top: 80rpx;
  box-sizing: border-box;
  height: calc(100vh - 186rpx);
}
.mine-modal .scroll-view {
  height: calc(100vh - 186rpx - 80rpx);
}
.mine-modal .close {
  width: 80rpx;
  height: 80rpx;
  top: -94rpx;
  text-align: center;
  line-height: 80rpx;
  position: absolute;
  left: 40rpx;
}
.mine-modal .close .icon_xiaocha {
  color: #ffffff;
  font-size: 30rpx;
}
.mine-modal .icon-image {
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  left: 80%;
  color: #F46BA1;
  width: 26rpx;
  height: 26rpx;
}
@keyframes modalopen {
  from {
    opacity: 0.5;
  }
  to {
    opacity: 1;
  }
}
