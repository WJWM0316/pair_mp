.account {
  background: #1F252B;
  min-height: 100vh;
  box-sizing: border-box;
}
.account .pitang-bg {
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  width: 100%;
}
.account .header {
  padding: 60rpx 60rpx 60rpx 48rpx;
  display: flex;
  align-items: center;
  box-sizing: border-box;
  height: 242rpx;
  position: relative;
}
.account .header .btn {
  width: 197rpx;
  height: 80rpx;
  background: #0CCDDA;
  border-radius: 44rpx;
  line-height: 80rpx;
  text-align: center;
  flex: 0 0 0 197rpx;
  font-size: 30rpx;
  font-weight: 700;
  color: #ffffff;
  padding: 0;
}
.account .header .inner {
  flex: 1;
  height: 140rpx;
  position: relative;
}
.account .header .pitang {
  height: 138rpx;
  display: inline-block;
}
.account .header .info {
  height: 140rpx;
  display: inline-block;
  vertical-align: top;
  padding-left: 18rpx;
}
.account .header .info .number {
  font-size: 86rpx;
  font-weight: 700;
  color: #ffffff;
  line-height: 96rpx;
  letter-spacing: 1px;
}
.account .header .info .entry {
  height: 37rpx;
  font-size: 26rpx;
  font-weight: 300;
  color: #ffffff;
  line-height: 37rpx;
  margin-top: 10rpx;
}
.account .header .info .icon {
  font-size: 20rpx;
  margin-left: 8rpx;
  position: relative;
  top: -2rpx;
}
.account .my-box {
  height: calc(100vh - 242rpx);
  background: #ffffff;
  box-shadow: 0rpx 7rpx 10rpx 0rpx rgba(197, 234, 226, 0.42);
  border-radius: 36rpx 36rpx 0 0 ;
  box-sizing: border-box;
  padding: 80rpx 60rpx;
  position: relative;
}
.account .my-header {
  position: relative;
}
.account .my-header .btn {
  width: 158rpx;
  height: 60rpx;
  border-radius: 31rpx;
  border: 2rpx solid #0CCDDA;
  line-height: 60rpx;
  text-align: center;
  font-size: 26rpx;
  font-weight: 700;
  color: #0CCDDA;
  position: absolute;
  top: 0;
  right: 0;
}
.account .my-header .btn2 {
  width: 158rpx;
  height: 60rpx;
  border-radius: 31rpx;
  border: 2rpx solid #CFD4D8;
  line-height: 60rpx;
  text-align: center;
  font-size: 26rpx;
  font-weight: 700;
  color: #9CA1AA;
  position: absolute;
  top: 0;
  right: 0;
}
.account .my-header .h1 {
  font-size: 36rpx;
  font-weight: 700;
  color: #1F252B;
  line-height: 44rpx;
}
.account .my-header .h2 {
  line-height: 32rpx;
  font-size: 24rpx;
  font-weight: 300;
  color: #9CA1AA;
  margin-top: 4rpx;
}
.account .ul {
  margin: 26rpx 0 40rpx 0;
  padding-bottom: 60rpx;
  position: relative;
}
.account .ul:after {
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  content: '';
  height: 1rpx;
  transform: scaleY(0.5);
  background: #e9eaec;
  display: block;
}
.account .ul:last-child {
  margin-left: 0;
}
.account .ul .li {
  width: 82rpx;
  margin-right: 9rpx;
  display: inline-block;
  box-sizing: border-box;
  position: relative;
}
.account .ul .li .item {
  width: 100%;
  height: 90rpx;
  background: rgba(241, 244, 244, 0.5);
  border-radius: 16rpx;
  box-sizing: border-box;
  position: relative;
  overflow: hidden;
  font-size: 28rpx;
  color: #9CA1AA;
  text-align: center;
  line-height: 90rpx;
}
.account .ul .li .day {
  font-size: 24rpx;
  color: #66666E;
  line-height: 32rpx;
  margin-top: 10rpx;
  width: 100%;
  text-align: center;
}
.account .ul .li .fanbei {
  position: absolute;
  left: 0;
  top: 0;
  width: 60rpx;
  height: 28rpx;
  background: #0CCDDA;
  border-radius: 0rpx 0rpx 16rpx 0rpx;
  font-size: 20rpx;
  color: #ffffff;
  text-align: center;
  line-height: 28rpx;
  z-index: 1;
}
.account .ul .li .sign-bg {
  position: absolute;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 100%;
}
.account .ul .li .txt1 {
  position: relative;
  font-weight: 700;
}
.account .ul .li .hasSign {
  color: #1F252B;
}
.account .my-header2 {
  position: relative;
  padding-bottom: 40rpx;
}
.account .my-header2:after {
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  content: '';
  height: 1rpx;
  transform: scaleY(0.5);
  background: #e9eaec;
  display: block;
}
.account .my-header2 .btn {
  width: 158rpx;
  height: 60rpx;
  border-radius: 31rpx;
  border: 2rpx solid #0CCDDA;
  line-height: 60rpx;
  text-align: center;
  font-size: 26rpx;
  font-weight: 700;
  color: #0CCDDA;
  position: absolute;
  top: 0;
  right: 0;
}
.account .my-header2 .h1 {
  font-size: 36rpx;
  font-weight: 700;
  color: #1F252B;
  line-height: 44rpx;
}
.account .my-header2 .h2 {
  line-height: 32rpx;
  font-size: 24rpx;
  font-weight: 300;
  color: #9CA1AA;
  margin-top: 4rpx;
}
.account .my-header2 text {
  font-size: 30rpx;
  color: #0CCDDA;
}
.drawer {
  width: 100vw;
  height: 100vh;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 10;
}
.drawer .content {
  width: 100vw;
  height: auto;
  border-radius: 24rpx 24rpx 0rpx 0rpx;
  position: absolute;
  bottom: 0;
  left: 0;
  box-sizing: border-box;
  z-index: 10;
  overflow: hidden;
  transition: all 0.4s linear;
  opacity: 0.5;
  background: #F1F4F4;
  z-index: 11;
}
.drawer .h1 {
  height: 50rpx;
  font-size: 36rpx;
  font-weight: 700;
  color: #1f252b;
  line-height: 50rpx;
  text-align: center;
  margin-top: 62rpx;
}
.drawer .h2 {
  font-size: 30rpx;
  color: #1f252b;
  line-height: 48rpx;
  position: relative;
  text-align: center;
  margin-top: 10rpx;
}
.drawer .inner11 {
  display: inline-block;
  position: relative;
}
.drawer .inner11:after {
  display: block;
  position: absolute;
  left: 0;
  right: 0;
  height: 24rpx;
  background: #0CCDDA;
  opacity: 0.08;
  bottom: 0;
  content: '';
}
.drawer .text11 {
  display: inline;
  position: relative;
  z-index: 2;
}
.drawer .my_pitang {
  height: 47rpx;
  position: relative;
  z-index: 2;
  top: 12rpx;
  margin-left: 5rpx;
}
.drawer .mask {
  width: 100vw;
  height: 100vh;
  position: fixed;
  top: 0;
  left: 0;
  background: rgba(0, 0, 0, 0.8);
  z-index: 9;
  animation-name: modalopen;
  animation-duration: 0.2s;
}
.drawer .mask.show {
  bottom: 0;
  opacity: 1;
}
.drawer .content.show {
  opacity: 1;
}
.drawer .icon_xiaocha {
  position: absolute;
  top: 0;
  right: 0;
  font-size: 20rpx;
  color: #CFD4D8;
  padding: 38rpx;
}
.drawer .tab {
  padding: 78rpx 198rpx 82rpx 198rpx;
  position: relative;
  overflow: hidden;
}
.drawer .tab .item1 {
  display: inline-block;
  float: left;
  width: unset;
  height: unset;
  border: unset;
  box-shadow: unset;
}
.drawer .tab .item1 .circle {
  width: 100rpx;
  height: 100rpx;
  background: #17C18A;
  border-radius: 50%;
  position: relative;
}
.drawer .tab .item2 {
  display: inline-block;
  float: right;
  width: unset;
  height: unset;
  border: unset;
  box-shadow: unset;
}
.drawer .tab .item2 .circle {
  width: 100rpx;
  height: 100rpx;
  background: #FCB81C;
  border-radius: 50%;
  position: relative;
}
.drawer .tab .txt {
  font-size: 26rpx;
  color: #1F252B;
  line-height: 38rpx;
  margin-top: 12rpx;
}
.drawer .tab .icon {
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
  font-size: 38rpx;
  color: #fff;
}
@keyframes modalopen {
  from {
    opacity: 0.5;
  }
  to {
    opacity: 1;
  }
}
