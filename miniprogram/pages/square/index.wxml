<navBar
  navBarBg="{{background}}"
  title="单身广场"
  userInfo="{{userInfo.userInfo}}"
  >
</navBar>
<view class="square">
  <view class="item" wx:for="{{ userData.list }}" wx:key="index" bindtap="routeJump" data-item="{{ item }}">
    <view class="cover">
      <i-image my-class="photo" src="{{ item.avatarInfo.url }}"></i-image>
      <view class="mask">
        <view class="album" bindtap="album">
          <i class="icon iconfont icon_im_tupian"></i>{{item.userAlbumList.length}}
        </view>
        <view class="info">
          <view class="row1">
            <view class="nickname">{{ item.nickname }}</view>
            <view class="year">{{ item.birthDesc }}</view>
            <view class="height">{{ item.heightDesc }}</view>
          </view>
          <view class="row2">
            <view class="city-area">{{ item.residentArr[1].title }}{{ item.residentArr[2].title }}</view>
            <view class="industry-position">{{ item.industryArr[0].name }} - {{ item.industryArr[1].name }}</view>
            <i-image my-class="icon-image" src="{{ CDNPATH }}icon_job_legalize.png" mode="widthFix" wx:if="{{item.isCareerIdentity}}"></i-image> 
          </view>
        </view>
      </view>
    </view>
    <view class="brief">
      {{ item.ownDescribe }}
    </view>
  </view>
</view>
<bottomStatus status="{{ onBottomStatus }}" listData="{{ userData.list }}"  marginTop="{{30}}" desc="已经到底啦！"></bottomStatus>
<tabBar tabBgColor="{{background}}"></tabBar>