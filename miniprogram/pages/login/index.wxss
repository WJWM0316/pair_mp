.loginWrap {
  padding-top: 374rpx;
  position: relative;
}
.loginWrap .header {
  position: absolute;
  top: 138rpx;
  left: 0%;
  width: 100%;
}
.loginWrap .header .logo {
  width: 222rpx;
  height: 74rpx;
  margin: 0 auto 12rpx;
  display: block;
}
.loginWrap .header .tips {
  text-align: center;
  font-size: 30rpx;
  font-weight: 300;
  color: #66666E;
  line-height: 42rpx;
  letter-spacing: 1rpx;
}
.loginWrap .footer {
  font-size: 24rpx;
  color: #9CA1AA;
  line-height: 33rpx;
  position: fixed;
  width: 100%;
  left: 0;
  bottom: 74rpx;
  text-align: center;
}
.loginWrap .footer .bold {
  color: #1F252B;
}
.loginWrap .box {
  width: 560rpx;
  height: 98rpx;
  background: #fff;
  border-radius: 49rpx;
  margin: 0 auto 24rpx;
  padding: 23rpx 50rpx 23rpx 112rpx;
  box-sizing: border-box;
  position: relative;
}
.loginWrap .box.codeBox {
  padding: 23rpx 190rpx 23rpx 112rpx;
}
.loginWrap .box .icon_shouji {
  font-size: 33rpx;
  position: absolute;
  left: 48rpx;
  top: 34rpx;
  color: #CFD4D8;
}
.loginWrap .box .icon_duanxin {
  font-size: 26rpx;
  position: absolute;
  left: 51rpx;
  top: 36rpx;
  color: #CFD4D8;
}
.loginWrap .box .input {
  width: 100%;
  height: 100%;
  font-size: 30rpx;
  color: #1F252B;
}
.loginWrap .box .send {
  position: absolute;
  top: 0;
  right: 50rpx;
  font-size: 28rpx;
  font-weight: 700;
  color: #0CCDDA;
  line-height: 99rpx;
  height: 100%;
}
.loginWrap .box .imgPath {
  width: 147rpx;
  height: 60rpx;
  position: absolute;
  top: 19rpx;
  right: 30rpx;
}
.loginWrap .loginBtn {
  width: 560rpx;
  height: 98rpx;
  background: #9FE9F0;
  border-radius: 49rpx;
  margin: 78rpx auto 0;
  color: #fff;
  font-size: 32rpx;
  border: none;
  line-height: 98rpx;
  box-shadow: none;
  font-weight: 400;
}
.loginWrap .loginBtn.canClick {
  background: #0CCDDA;
}
.loginWrap .wxLogin {
  width: 100%;
  text-align: center;
  font-size: 32rpx;
  color: #17C18A;
  line-height: 40rpx;
  letter-spacing: 1rpx;
  background: none;
  position: fixed;
  bottom: 213rpx;
  font-weight: 400;
}
.loginWrap .wxLogin .icon {
  font-size: 32rpx;
  margin-right: 18rpx;
}
