.user-page {
  margin: 20rpx 30rpx;
}
.user-page .ul {
  margin-bottom: 24rpx;
  background: #fff;
  border-radius: 16rpx;
  position: relative;
  overflow: hidden;
  padding-bottom: 6rpx;
  padding-top: 6rpx;
}
.user-page .key {
  font-size: 30rpx;
  color: #1F252B;
}
.user-page .placeholder {
  position: absolute;
  right: 0;
  left: 0;
  top: 48%;
  transform: translateY(-50%);
}
.user-page .ul-album {
  height: 216rpx;
  background: #fff;
  display: flex;
  align-items: center;
  padding-right: 70rpx;
  padding-left: 40rpx;
  padding-bottom: 0;
  padding-top: 0;
}
.user-page .album-box {
  width: 170rpx;
  height: 170rpx;
  border-radius: 16rpx;
  background: rgba(0, 0, 0, 0.05);
  position: absolute;
  right: 72rpx;
  overflow: hidden;
}
.user-page .album-box .tips {
  position: absolute;
  right: 15rpx;
  top: 15rpx;
  font-size: 24rpx;
  color: white;
}
.user-page .album-box .icon {
  font-size: 24rpx;
  color: white;
  margin-right: 5rpx;
}
.user-page .album-box .avatarImg {
  width: 100%;
  height: 100%;
}
.user-page .icon-1 {
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  right: 32rpx;
  top: 50%;
  transform: translateY(-50%);
  line-height: 1.11;
  color: #CFD4D8;
  font-size: 28rpx;
}
.user-page .icon-2 {
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  right: 32rpx;
  top: 40rpx;
  line-height: 1.11;
  color: #CFD4D8;
  font-size: 28rpx;
}
.user-page .value {
  font-size: 30rpx;
  color: #66666E;
  width: 420rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  display: inline-block;
  vertical-align: middle;
  text-align: right;
  line-height: 102rpx;
  position: relative;
}
.user-page .label {
  display: inline-block;
  width: calc(100% - 420rpx);
  vertical-align: middle;
}
.user-page .base-info .li,
.user-page .society .li {
  height: 108rpx;
  line-height: 108rpx;
  position: relative;
  padding-left: 40rpx;
  padding-right: 70rpx;
  display: flex;
}
.user-page .base-info .btn-status,
.user-page .society .btn-status {
  display: inline-block;
  width: 96rpx;
  height: 40rpx;
  border-radius: 20rpx;
  line-height: 40rpx;
  text-align: center;
  font-size: 22rpx;
  font-weight: 700;
  color: #fff;
}
.user-page .base-info .status1,
.user-page .society .status1 {
  background: #0CCDDA;
}
.user-page .base-info .status2,
.user-page .society .status2 {
  background: #E85274;
}
.user-page .base-info .status3,
.user-page .society .status3 {
  background: #E85274;
}
.user-page .base-info .status4,
.user-page .society .status4 {
  background: #CFD4D8;
}
.user-page .base-info .limit-company-name,
.user-page .society .limit-company-name {
  max-width: 300rpx;
  display: inline-block;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
  line-height: 1;
  vertical-align: middle;
  margin-right: 14rpx;
}
.user-page .base-info .gray,
.user-page .society .gray {
  font-size: 30rpx;
  color: #cfd4d8;
}
.user-page .func-btn {
  width: 182rpx;
  height: 64rpx;
  border-radius: 33rpx;
  border: 2rpx solid #0CCDDA;
  line-height: 64rpx;
  text-align: center;
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  right: 40rpx;
  font-size: 28rpx;
  color: #0CCDDA;
}
.user-page .desc {
  font-size: 30rpx;
  color: #66666E;
  line-height: 40rpx;
  margin: -16rpx 70rpx 50rpx 40rpx;
  display: block;
}
.user-page .description,
.user-page .ideal {
  padding-top: 0;
  padding-bottom: 0;
}
.user-page .description .li,
.user-page .ideal .li {
  line-height: 1;
  position: relative;
  display: flex;
  padding: 36rpx 70rpx 36rpx 40rpx;
}
.user-page .myLabel {
  padding-top: 0;
  padding-bottom: 0;
}
.user-page .myLabel .li {
  line-height: 1;
  position: relative;
  display: flex;
  padding: 36rpx 70rpx 36rpx 40rpx;
}
.user-page .myLabel .row {
  position: relative;
  margin-bottom: 10px;
  display: flex;
  margin: 0 40rpx 20rpx;
}
.user-page .myLabel .row:last-child {
  margin-bottom: 40rpx;
}
.user-page .myLabel .type {
  width: 54rpx;
  height: 54rpx;
  background: #F1F4F4;
  border-radius: 50%;
  margin-right: 22rpx;
  display: inline-block;
  flex: 0 0 27px;
  position: relative;
  text-align: center;
  line-height: 54rpx;
}
.user-page .myLabel .type .icon {
  font-size: 28rpx;
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
  color: #1F252B;
}
.user-page .myLabel .item {
  height: 56rpx;
  background: #F1F4F4;
  border-radius: 28rpx;
  padding: 0 30rpx;
  display: inline-block;
  margin-right: 14rpx;
  margin-bottom: 12rpx;
  font-size: 28rpx;
  color: #1F252B;
  line-height: 56rpx;
  text-align: center;
}
.user-page .answer {
  padding-top: 0;
  padding-bottom: 0;
}
.user-page .answer .li {
  line-height: 1;
  position: relative;
  display: flex;
  padding: 36rpx 70rpx 36rpx 40rpx;
}
.user-page .answer .my-icon {
  display: inline-block;
  position: relative;
  width: 14rpx;
  height: 21rpx;
  margin-right: 12rpx;
}
.user-page .answer .my-icon:before {
  width: 100%;
  height: 100%;
  background: #1F252B;
  content: '';
  display: block;
  position: absolute;
  left: 0;
  right: 0;
  z-index: 1;
}
.user-page .answer .my-icon:after {
  width: 200%;
  height: 200%;
  background: #fff;
  content: '';
  display: block;
  border: 1rpx solid #1F252B;
  position: absolute;
  top: -14rpx;
  right: -10rpx;
  box-sizing: border-box;
  transform: scale(0.5, 0.5);
}
.user-page .answer .h1 {
  font-size: 30rpx;
  color: #66666E;
  line-height: 38rpx;
  position: relative;
  padding-right: 70rpx;
}
.user-page .answer .h2 {
  font-size: 30rpx;
  color: #9CA1AA;
  line-height: 38rpx;
  margin-top: 12rpx;
  padding-right: 70rpx;
}
.user-page .answer .item {
  padding-left: 40rpx;
  position: relative;
  padding-top: 32rpx;
  padding-bottom: 32rpx;
}
.user-page .answer .item:after {
  position: absolute;
  left: 40rpx;
  right: 40rpx;
  bottom: 0;
  content: '';
  height: 1rpx;
  transform: scaleY(0.5);
  background: #EDF1F0;
  display: block;
}
.user-page .answer .item:last-child {
  padding-bottom: 52rpx;
}
.user-page .answer .box {
  margin-top: 4rpx;
}
.holderStyle {
  font-size: 30rpx;
  color: #CFD4D8;
}
